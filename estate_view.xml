                                    <field name="partner_ids" widget="many2many_tags_email" />
                                </div>
                                <field name="subject" placeholder="Subject..."/>
                            </group>
                            <field name="body" style="border: 1pt solid #cccccc;" />
                        </form>
                        <tree string="EMails" create="false" delete="false">
