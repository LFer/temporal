                                <field name="terraza_2" nolabel="0"/>
                                <field name="azotea" nolabel="0"/>
                                <field name="porteria_2" nolabel="0"/>
                                <field name="vigilancia" nolabel="0"/>
                                <field name="porteria" nolabel="0"/>
                                <field name="ascensor" nolabel="0"/>
                                <field name="piso" nolabel="0"/>
                                <field name="internet" nolabel="0"/>
                                <field name="sauna" nolabel="0"/>
                                <field name="gym" nolabel="0"/>
                                <field name="canchas" nolabel="0"/>
                                <field name="bbq" nolabel="0"/>
                            </group>
                            <group string="Descripción Exterior">
                                <field name="baulera" nolabel="0"/>
                                <field name="fondo" nolabel="0"/>
                                <field name="jardin" nolabel="0"/>
                                <field name="piscina" nolabel="0"/>
                                <field name="barbacoa" nolabel="0"/>
                            </group>
                        </group>
	            </page>

	            <page string="Documentación">
	                <group>
	                    <field name="documentacion" />
	                    <field name="escribano" placeholder="Nombre del escribano..." style="width: 300px " />
	                </group>        
				</page>
		        
                <page string="Tasación">
                    <group>
                        <field name="fecha_tasacion" nolabel="0" style="width: 17%%" />
                        <field name="moneda_tasacion" nolabel="0" style="width: 17%%" domain="['|',( 'name','=','USD' ),( 'name','=','UYP' )]"/>
                        <field name="importe_tasacion" nolabel="0" style="width: 15%% " />
                        <field name="tasado_por" nolabel="0" style="width: 20%% " />
                    </group>
                </page>
              
		        <page string="Condiciones de Venta" >                    
                    <group>
                        <h3>
                            <label for="currency" style="color:red"/>
                    	    <field name="currency" class="oe_inline"  style="width: 17%%" domain="['|',( 'name','=','USD' ),( 'name','=','UYP' )]" nolabel="1" />
                    	    <!--field name="currency_al" style="width: 17%%" domain="['|',( 'name','=','USD' ),( 'name','=','UYP' )]" attrs="{'invisible': [('alquiler','=',False)]}"/-->
                    	
                            <label for="price"  style="color:red" />
                            <field name="price" class="oe_inline" widget='monetary' nolabel="1" />
                        </h3>
                    </group>
                    <group>
                        
                        <!--label for="rent_price" style="color:red" attrs="{'invisible': [('alquiler','=', False)]}"/>
                        <field name="rent_price" attrs="{'invisible': [('alquiler','=', False)]}" nolabel="1"/-->
                        
                        <!--label for="rent_day" style="color:red" attrs="{'invisible': [('alquiler','=', False)]}"/>
                        <field name="rent_day" attrs="{'invisible': [('alquiler','=', False)]}" nolabel="1"/-->                        
                        
                        <!--field name="fecha_inicio" on_change="get_number_of_days(fecha_inicio,fecha_fin, rent_day)" style="width: 17%%" nolabel="0" attrs="{'invisible': [('alquiler','=', False)]}"/>
                        <field name="fecha_fin" on_change="get_number_of_days(fecha_inicio,fecha_fin, rent_day)" style="width: 17%%" nolabel="0" attrs="{'invisible': [('alquiler','=', False)]}"/>
                        <field name="result"  style="width: 17%%" nolabel="0" attrs="{'invisible': [('alquiler','=', False)]}"/>
                        <field name="costo_alquiler" style="width: 17%%" nolabel="0" attrs="{'invisible': [('alquiler','=', False)]}"/-->


                        <field name="conditions"/>
                        <field name="financiacion"/>

                		<field name="precioXHa" attrs="{'invisible': [('is_rural','=', False)]}"/>                        
                		<field name="precioTotalLiquido" attrs="{'invisible': [('is_rural','=', False)]}"/>
                		<label for="precioXHaComIncl" style="color:red" attrs="{'invisible': [('is_rural','=', False)]}" />
	                    <field name="precioXHaComIncl" attrs="{'invisible': [('is_rural','=', False)]}" nolabel="1" />        
	                    <label for="precioTotalLiquidoComIncl" style="color:red" attrs="{'invisible': [('is_rural','=', False)]}" />
                    	<field name="precioTotalLiquidoComIncl" attrs="{'invisible': [('is_rural','=', False)]}" nolabel="1" />

                    </group>
                    <button name="action_calcular_precio_hectarea" type="object" string="Calcular precio hectárea" class="oe_highlight" attrs="{'invisible': [('is_rural','=', False)]}"/>                        	            
	            </page>


                
                
                    <page string="Alquiler por temporada" attrs="{'invisible': [('alquiler','=', False)]}" >
                
                    <field name="temporada_ids" widget="one2many_list" mode="tree">
                
                        <tree string="Alquiler por temporada" editable="bottom">
                
                            <field name="cliente_1"/>
                            <field name="usuario_1"/>        
                            <field name="currency_al" domain="['|',( 'name','=','USD' ),( 'name','=','UYP' )]" />        
                            <field name="fecha_inicio" on_change="get_number_of_days(fecha_inicio,fecha_fin, rent_day)" />        
                            <field name="fecha_fin" on_change="get_number_of_days(fecha_inicio,fecha_fin, rent_day)" />        
                            <field name="rent_day"/>
                            <field name="rent_price"/>    
                            <field name="result"/>        
                            <field name="costo_alquiler"/>      
                            <field name="pago_1"/>        
                
                        </tree>
                    </field>
                </page>
                

	            
	            <page string="Características" attrs="{'invisible': [('is_rural','=', False)]}">
	                <group col="4" >

  	            
                        <label for="distancia" style="color:red" />
	                    <field name="distancia" nolabel="1" />
	                    <label for="accesos" style="color:red" />
	                   	<field name="accesos" nolabel="1" />
	                   	<label for="superficie" style="color:red" />
	                    <field name="superficie" nolabel="1" />
	                    
	                    <label for="indiceConeat" style="color:red" />
	                    <field name="indiceConeat" nolabel="1" />
						<!-- <field name="indiceProdFinal" /> -->
						
	                    <field name="indiceValorReal" />
                        <label for="exploit" style="color:red" />
	                    <field name="exploit" nolabel="1" />
	                    <label for="montes" style="color:red" />
	                    <field name="montes" nolabel="1" />
	                    <label for="supForestada" style="color:red" />
	                    <field name="supForestada" nolabel="1" />

	                    <field name="padrones"  />

	                </group>  
				</page>
				
				
				<page string="Personas relacionadas" >
